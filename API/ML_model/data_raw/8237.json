io natural y cultural de su\r\n\r\ncomunidad\r\n", "documentoProyecto": "ProyectoEducativo8237.pdf", "documentoReglamento": "ReglamentodeConvivencia8237.pdf", "internado": false, "integracion": false, "subvencionPreferencial": true, "peib": true, "politicaUniforme": "Uniforme propio", "orientacionReligiosa": "Laica", "alumnosMatriculados": 4, "promedioAlumnosPorCurso": 1, "cantidadDocentes": 2, "regimen": "Mixto"}, "sedes": [{"codigoSede": 1, "direccion": {"codigoRegion": 10, "codigoComuna": 10207, "region": "De Los Lagos", "comuna": "Queil\u00e9n", "calle": "PAILDAD S/N", "coordenadas": {"type": "Point", "coordinates": [-73.60306, -42.85803]}}, "niveles": [{"codigoCurso": 111000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n B\u00e1sica", "glosaEnsenanza": "Ense\u00f1anza B\u00e1sica", "glosaNivel": "1\u00b0 b\u00e1sico", "etiquetaNivel": "1BASICO", "sexo": "MIXTO", "glosaJornada": "COMPLETA", "glosaEspecialidad": null, "glosaGrupoPago": "BASICA", "codigoEnsenanza": 110, "codigoNivel": 1, "codigoJornada": 3, "codigoSexo": 3, "codigoEspecialidad": 0, "unicoComuna": true, "proporcionExcelenciaTransicion": null, "proporcionExcelenciaRegimen": null, "proporcionEspecializacionTemprana": null, "copago": {"numeroCuotas": 0, "valorCuota": 0}, "cuposTotales": 5, "numeroVacantesRangoInferior": 5, "numeroVacantesRangoSuperior": 5, "porcentajeCambioInferior": 0.0, "procentajeCambioSuperior": 0.0, "cantidadRepitentesAnioActual": 0, "cantidadRepitentesNivelAnterior": 0, "cantidadPreInscritosAnioSiguiente": 0, "cambiosInferior": 0, "cambiosSuperior": 0, "cantidadPreVacantesInferior": 5, "cantidadPreVacantesSuperior": 5, "rango": 0, "numeroPostulantesAnioAnterior": 0, "numeroMovimientoListaEsperaAnoAnterior": 0}, {"codigoCurso": 211000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n B\u00e1sica", "glosaEnsenanza": "Ense\u00f1anza B\u00e1sica", "glosaNivel": "2\u00b0 b\u00e1sico", "etiquetaNivel": "2BASICO", "sexo": "MIXTO", "glosaJornada": "COMPLETA", "glosaEspecialidad": null, "glos