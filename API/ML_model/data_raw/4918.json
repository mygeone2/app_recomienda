s contiene los valores que establecen la identidad que constituyen el bien com\u00fan por el cual se trabajar\u00e1. ", "documentoProyecto": "ProyectoEducativo4918.pdf", "documentoReglamento": "ReglamentodeConvivencia4918.pdf", "internado": false, "integracion": true, "subvencionPreferencial": true, "peib": false, "politicaUniforme": "Uniforme regular", "orientacionReligiosa": "Laica", "alumnosMatriculados": 14, "promedioAlumnosPorCurso": 2, "cantidadDocentes": 3, "regimen": "Mixto"}, "sedes": [{"codigoSede": 1, "direccion": {"codigoRegion": 8, "codigoComuna": 8105, "region": "Del Biob\u00edo", "comuna": "Hualqui", "calle": "CAMINO HUALQUI-YUMBEL KM 5 S/N", "coordenadas": {"type": "Point", "coordinates": [-72.90092, -36.97743]}}, "niveles": [{"codigoCurso": 111000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n B\u00e1sica", "glosaEnsenanza": "Ense\u00f1anza B\u00e1sica", "glosaNivel": "1\u00b0 b\u00e1sico", "etiquetaNivel": "1BASICO", "sexo": "MIXTO", "glosaJornada": "COMPLETA", "glosaEspecialidad": null, "glosaGrupoPago": "BASICA", "codigoEnsenanza": 110, "codigoNivel": 1, "codigoJornada": 3, "codigoSexo": 3, "codigoEspecialidad": 0, "unicoComuna": true, "proporcionExcelenciaTransicion": null, "proporcionExcelenciaRegimen": null, "proporcionEspecializacionTemprana": null, "copago": {"numeroCuotas": 0, "valorCuota": 0}, "cuposTotales": 5, "numeroVacantesRangoInferior": 5, "numeroVacantesRangoSuperior": 5, "porcentajeCambioInferior": 0.0, "procentajeCambioSuperior": 0.0, "cantidadRepitentesAnioActual": 0, "cantidadRepitentesNivelAnterior": 0, "cantidadPreInscritosAnioSiguiente": 0, "cambiosInferior": 0, "cambiosSuperior": 0, "cantidadPreVacantesInferior": 5, "cantidadPreVacantesSuperior": 5, "rango": 0, "numeroPostulantesAnioAnterior": 0, "numeroMovimientoListaEsperaAnoAnterior": 0}, {"codigoCurso": 211000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n B\u00e1sica", "glosaEnsenanza": "Ense\u00f1anza B\u00e1sica", "glosaNivel": "2\u00b0 b\u00e1sico", "etiquetaNivel": "2