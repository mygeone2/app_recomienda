aci\u00f3n y proyecci\u00f3n del mismo.                    ", "documentoProyecto": "ProyectoEducativo5205.pdf", "documentoReglamento": "ReglamentodeConvivencia5205.pdf", "internado": false, "integracion": true, "subvencionPreferencial": true, "peib": true, "politicaUniforme": "Uniforme regular", "orientacionReligiosa": "Laica", "alumnosMatriculados": 54, "promedioAlumnosPorCurso": 6, "cantidadDocentes": 8, "regimen": "Mixto"}, "sedes": [{"codigoSede": 1, "direccion": {"codigoRegion": 8, "codigoComuna": 8207, "region": "Del Biob\u00edo", "comuna": "Tir\u00faa", "calle": "CAMINO CA\u00d1ETE TIR\u00daA KM 43 S/N", "coordenadas": {"type": "Point", "coordinates": [-73.37408, -38.1879]}}, "niveles": [{"codigoCurso": 111000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n B\u00e1sica", "glosaEnsenanza": "Ense\u00f1anza B\u00e1sica", "glosaNivel": "1\u00b0 b\u00e1sico", "etiquetaNivel": "1BASICO", "sexo": "MIXTO", "glosaJornada": "COMPLETA", "glosaEspecialidad": null, "glosaGrupoPago": "BASICA", "codigoEnsenanza": 110, "codigoNivel": 1, "codigoJornada": 3, "codigoSexo": 3, "codigoEspecialidad": 0, "unicoComuna": true, "proporcionExcelenciaTransicion": null, "proporcionExcelenciaRegimen": null, "proporcionEspecializacionTemprana": null, "copago": {"numeroCuotas": 0, "valorCuota": 0}, "cuposTotales": 10, "numeroVacantesRangoInferior": 10, "numeroVacantesRangoSuperior": 10, "porcentajeCambioInferior": 0.0, "procentajeCambioSuperior": 0.0, "cantidadRepitentesAnioActual": 0, "cantidadRepitentesNivelAnterior": 0, "cantidadPreInscritosAnioSiguiente": 0, "cambiosInferior": 0, "cambiosSuperior": 0, "cantidadPreVacantesInferior": 10, "cantidadPreVacantesSuperior": 10, "rango": 0, "numeroPostulantesAnioAnterior": 0, "numeroMovimientoListaEsperaAnoAnterior": 0}, {"codigoCurso": 211000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n B\u00e1sica", "glosaEnsenanza": "Ense\u00f1anza B\u00e1sica", "glosaNivel": "2\u00b0 b\u00e1sico", "etiquetaNivel": "2BASICO", "sexo": "MIXTO", "glosaJornada": "