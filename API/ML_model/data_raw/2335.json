ta y acogedora.\r\n", "documentoProyecto": "ProyectoEducativo2335.pdf", "documentoReglamento": "ReglamentodeConvivencia2335.pdf", "internado": false, "integracion": true, "subvencionPreferencial": true, "peib": false, "politicaUniforme": "Uniforme propio", "orientacionReligiosa": "Laica", "alumnosMatriculados": 69, "promedioAlumnosPorCurso": 6, "cantidadDocentes": 19, "regimen": "Mixto"}, "sedes": [{"codigoSede": 1, "direccion": {"codigoRegion": 6, "codigoComuna": 6117, "region": "Del Libertador Bernardo Ohiggins", "comuna": "San Vicente", "calle": "CAMINO EL CURA S/N, TUNCA ABAJO PARADERO 26 CARRETERA DE LA FRUTA HACIA EL SUR S/N", "coordenadas": {"type": "Point", "coordinates": [-71.11694, -34.41874]}}, "niveles": [{"codigoCurso": 401000000131, "glosaGrupoEnsenanza": "Educaci\u00f3n Parvularia", "glosaEnsenanza": "Educaci\u00f3n Parvularia", "glosaNivel": "1er nivel de Transici\u00f3n (Pre-kinder)", "etiquetaNivel": "PREKINDER", "sexo": "MIXTO", "glosaJornada": "MA\u00d1ANA", "glosaEspecialidad": null, "glosaGrupoPago": "PARVULARIA", "codigoEnsenanza": 10, "codigoNivel": 4, "codigoJornada": 1, "codigoSexo": 3, "codigoEspecialidad": 0, "unicoComuna": true, "proporcionExcelenciaTransicion": null, "proporcionExcelenciaRegimen": null, "proporcionEspecializacionTemprana": null, "copago": {"numeroCuotas": 0, "valorCuota": 0}, "cuposTotales": 40, "numeroVacantesRangoInferior": 40, "numeroVacantesRangoSuperior": 40, "porcentajeCambioInferior": 0.0, "procentajeCambioSuperior": 0.0, "cantidadRepitentesAnioActual": 0, "cantidadRepitentesNivelAnterior": 0, "cantidadPreInscritosAnioSiguiente": 0, "cambiosInferior": 0, "cambiosSuperior": 0, "cantidadPreVacantesInferior": 40, "cantidadPreVacantesSuperior": 40, "rango": 0, "numeroPostulantesAnioAnterior": 0, "numeroMovimientoListaEsperaAnoAnterior": 0}, {"codigoCurso": 501000000131, "glosaGrupoEnsenanza": "Educaci\u00f3n Parvularia", "glosaEnsenanza": "Educaci\u00f3n Parvularia", "glosaNivel": "2\u00b0 nivel de Transici\u00f3n (K