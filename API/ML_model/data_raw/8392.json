 higiene personal.", "documentoProyecto": "ProyectoEducativo8392.pdf", "documentoReglamento": "ReglamentodeConvivencia8392.pdf", "internado": false, "integracion": true, "subvencionPreferencial": true, "peib": true, "politicaUniforme": "Uniforme propio", "orientacionReligiosa": "Laica", "alumnosMatriculados": 285, "promedioAlumnosPorCurso": 20, "cantidadDocentes": 32, "regimen": "Mixto"}, "sedes": [{"codigoSede": 1, "direccion": {"codigoRegion": 11, "codigoComuna": 11203, "region": "De Ays\u00e9n Del General Carlos Iba\u00f1ez Del Campo", "comuna": "Guaitecas", "calle": "COSTANERA S/N S/N", "coordenadas": {"type": "Point", "coordinates": [-73.74857, -43.89654]}}, "niveles": [{"codigoCurso": 401000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n Parvularia", "glosaEnsenanza": "Educaci\u00f3n Parvularia", "glosaNivel": "1er nivel de Transici\u00f3n (Pre-kinder)", "etiquetaNivel": "PREKINDER", "sexo": "MIXTO", "glosaJornada": "COMPLETA", "glosaEspecialidad": null, "glosaGrupoPago": "PARVULARIA", "codigoEnsenanza": 10, "codigoNivel": 4, "codigoJornada": 3, "codigoSexo": 3, "codigoEspecialidad": 0, "unicoComuna": true, "proporcionExcelenciaTransicion": null, "proporcionExcelenciaRegimen": null, "proporcionEspecializacionTemprana": null, "copago": {"numeroCuotas": 0, "valorCuota": 0}, "cuposTotales": 25, "numeroVacantesRangoInferior": 11, "numeroVacantesRangoSuperior": 11, "porcentajeCambioInferior": 0.0, "procentajeCambioSuperior": 0.0, "cantidadRepitentesAnioActual": 0, "cantidadRepitentesNivelAnterior": 0, "cantidadPreInscritosAnioSiguiente": 0, "cambiosInferior": 0, "cambiosSuperior": 0, "cantidadPreVacantesInferior": 25, "cantidadPreVacantesSuperior": 25, "rango": 0, "numeroPostulantesAnioAnterior": 2, "numeroMovimientoListaEsperaAnoAnterior": 0}, {"codigoCurso": 501000000133, "glosaGrupoEnsenanza": "Educaci\u00f3n Parvularia", "glosaEnsenanza": "Educaci\u00f3n Parvularia", "glosaNivel": "2\u00b0 nivel de Transici\u00f3n (Kinder)", "etiquetaNivel": "KINDER", "sexo": "M