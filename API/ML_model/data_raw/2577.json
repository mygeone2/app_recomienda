 b\u00e1sico.\r\n", "documentoProyecto": "ProyectoEducativo2577.pdf", "documentoReglamento": "ReglamentodeConvivencia2577.pdf", "internado": false, "integracion": true, "subvencionPreferencial": true, "peib": false, "politicaUniforme": "Uniforme regular", "orientacionReligiosa": "Laica", "alumnosMatriculados": 76, "promedioAlumnosPorCurso": 7, "cantidadDocentes": 11, "regimen": "Mixto"}, "sedes": [{"codigoSede": 1, "direccion": {"codigoRegion": 6, "codigoComuna": 6310, "region": "Del Libertador Bernardo Ohiggins", "comuna": "Santa Cruz", "calle": "CAMINO A CHEPICA KM. 15- EL BOLDAL S/N", "coordenadas": {"type": "Point", "coordinates": [-71.30978, -34.67625]}}, "niveles": [{"codigoCurso": 501000000131, "glosaGrupoEnsenanza": "Educaci\u00f3n Parvularia", "glosaEnsenanza": "Educaci\u00f3n Parvularia", "glosaNivel": "2\u00b0 nivel de Transici\u00f3n (Kinder)", "etiquetaNivel": "KINDER", "sexo": "MIXTO", "glosaJornada": "MA\u00d1ANA", "glosaEspecialidad": null, "glosaGrupoPago": "PARVULARIA", "codigoEnsenanza": 10, "codigoNivel": 5, "codigoJornada": 1, "codigoSexo": 3, "codigoEspecialidad": 0, "unicoComuna": true, "proporcionExcelenciaTransicion": null, "proporcionExcelenciaRegimen": null, "proporcionEspecializacionTemprana": null, "copago": {"numeroCuotas": 0, "valorCuota": 0}, "cuposTotales": 11, "numeroVacantesRangoInferior": 3, "numeroVacantesRangoSuperior": 3, "porcentajeCambioInferior": 0.0, "procentajeCambioSuperior": 0.2896825396, "cantidadRepitentesAnioActual": 0, "cantidadRepitentesNivelAnterior": 0, "cantidadPreInscritosAnioSiguiente": 7, "cambiosInferior": 0, "cambiosSuperior": 2, "cantidadPreVacantesInferior": 4, "cantidadPreVacantesSuperior": 7, "rango": 2, "numeroPostulantesAnioAnterior": 1, "numeroMovimientoListaEsperaAnoAnterior": 0}, {"codigoCurso": 401000000131, "glosaGrupoEnsenanza": "Educaci\u00f3n Parvularia", "glosaEnsenanza": "Educaci\u00f3n Parvularia", "glosaNivel": "1er nivel de Transici\u00f3n (Pre-kinder)", "etiquetaNivel": "PREKINDER", "sexo